{
    "workbench.startupEditor": "newUntitledFile",
    "beautify.ignore": ["**/*.js"],
    "eslint.enable": false,
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "files.associations": {
        "*.mustache": "html",
        "*.pyx": "python",
        "*.py.jinja2": "python"
    },
    "files.exclude": {
        "**/*.pyc": true,
        "**/package-lock.json": true,
        "**/bower_components": true,
        "**/node_modules": true,
        "**/__pycache__": true,
        "**/env": true
    },
    "python.formatting.provider": "black",
    "python.linting.flake8Enabled": false,
    "python.linting.pylintEnabled": false,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "editor.renderWhitespace": "all",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "GitHub Plus",
    "vetur.format.defaultFormatter.js": "none",
    "emmet.includeLanguages": {
        "edge": "html",
        "vue-html": "html",
        "vue": "html"
    },
    "editor.wordWrap": "on",
    "editor.rulers": [88],
    "typescript.validate.enable": false,
    "javascript.validate.enable": false,
    "typescript.suggestionActions.enabled": false,
    "breadcrumbs.enabled": true,
    "git.confirmSync": false,
    "editor.minimap.enabled": false,
    "vetur.format.options.tabSize": 4,
    "go.lintTool": "golint",
    "go.formatTool": "gofmt",
    "go.gopath": "/home/gio/Documents/golang/packages",
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.opacity": 1,
    "C_Cpp.default.cppStandard": "c++14",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "C_Cpp.updateChannel": "Insiders",
    "update.mode": "none",
    "prettier.tabWidth": 4,
    "latex-workshop.bind.enter.key": false,
    "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": false,
    "latex-workshop.latex.build.clearLog.everyRecipeStep.enabled": false,
    "latex-workshop.latex.option.maxPrintLine.enabled": false,
    "latex-workshop.latex.rootFile.useSubFile": false,
    "latex-workshop.message.badbox.show": false,
    "latex-workshop.message.convertFilenameEncoding": false,
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.log.show": false,
    "latex-workshop.message.update.show": false,
    "latex-workshop.message.warning.show": false,
    "latex-workshop.progress.enabled": false,
    "latex-workshop.latex.autoBuild.run": "never",
    "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "terminal.integrated.allowChords": false,
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "github.gitAuthentication": false
}
